{ 
"manifest_version": 2,
"name": "DeterBrowser"
, "version": "0",
"permissions": [
        "activeTab","webRequest","webRequestBlocking","downloads",
        "tabs", "http://*/*", "https://*/*","webNavigation", "*://*/*"
    ],
 "content_scripts":
    [{ "matches": ["file:///*/*", "http://*/*"]
     , "run_at": "document_start"
     , "js": ["content.js"]
     }]
}
